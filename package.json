{
  "name": "gcode-replay",
  "version": "0.0.1",
  "description": "Video-style scrubber for G-code files.",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "test": "npm run test:unit",
    "test:unit": "mocha -r ts-node/register test/unit/*.test.ts",
    "trial": "npm start -- ../test/sled-0.25-in.nc",
    "inspect:test:unit": "mocha --inspect-brk -r ts-node/register test/unit/*.test.ts",
    "build": "./node_modules/.bin/webpack --config ./viewer.webpack.config.js",
    "watch": "./node_modules/.bin/webpack --config ./viewer.webpack.config.js --watch --progress"
  },
  "keywords": [
    "typescript",
    "gcode",
    "g-code",
    "replay"
  ],
  "author": "Tim Bright",
  "license": "MIT",
  "dependencies": {
    "codemirror": "^5.48.2",
    "minimist": "^1.2.0",
    "react": "^16.9.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.9.0",
    "react-sizeme": "^2.6.7",
    "rxjs": "^6.5.2",
    "styled-components": "^4.3.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/codemirror": "0.0.76",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.3",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "canopy": "^0.3.0",
    "chai": "^4.2.0",
    "css-loader": "^3.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.1.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  }
}
